<Application
    x:Class="ChessViewer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:ChessViewer.Converters"
    xmlns:local="using:ChessViewer">
    <Application.Resources>
        <!-- Conversores -->
        <converter:FileIconConverter x:Key="FileIconConverter" />
        <converter:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
        <converter:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter" />
        <converter:MvvmColorConverter x:Key="MvvmColorConverter" />
    </Application.Resources>
</Application>
